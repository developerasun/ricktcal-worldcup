{
  "name": "ricktcal-worldcup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dep": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "gen:type": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "db:schema": "pnpm exec drizzle-kit generate",
    "db:local": "wrangler d1 execute ricktcal-db --local --file=./src/server/database/migrations/0001_nasty_salo.sql",
    "db:remote": "wrangler d1 execute ricktcal-db --remote --file=./src/server/database/migrations/sync.sql",
    "cron:dev": "curl --request POST  --url 'http://localhost:3000/api/webhook'  --header 'Authorization: Bearer 0580907ac1f77f186c3b553aabb1a82bfefd2fca15d09f1d0c393c47ec8c8640'"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.5.3",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-spring/web": "^10.0.1",
    "@use-gesture/react": "^10.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.3",
    "es-toolkit": "^1.39.8",
    "ethers": "^6.15.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "wrangler": "^4.25.1"
  }
}
