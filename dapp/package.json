{
  "name": "ricktcal-worldcup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm apidoc && next dev --turbopack --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "apidoc": "node --experimental-strip-types apispec.ts",
    "dep": "pnpm apidoc && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "pnpm apidoc && opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "gen:type": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "db:schema": "pnpm exec drizzle-kit generate",
    "db:local": "wrangler d1 execute ricktcal-db --local --file=./src/server/database/migrations/0003_mature_prowler.sql",
    "db:remote": "wrangler d1 execute ricktcal-db --remote --file=./src/server/database/migrations/0003_mature_prowler.sql"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.5.3",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-spring/web": "^10.0.1",
    "@reactour/tour": "^3.8.0",
    "@use-gesture/react": "^10.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.3",
    "es-toolkit": "^1.39.8",
    "ethers": "^6.15.0",
    "jose": "^6.0.12",
    "lucide-react": "^0.525.0",
    "nanoid": "^5.1.5",
    "next": "15.3.5",
    "next-swagger-doc": "^0.4.1",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.6",
    "swagger-ui-react": "^5.27.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "drizzle-kit": "^0.31.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "wrangler": "^4.25.1"
  }
}
